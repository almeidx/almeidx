{
  "$schema": "http://json.schemastore.org/package",
  "name": "website",
  "homepage": "https://almeidx.dev",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ext .ts,.tsx --cache src",
    "lint:fix": "eslint --ext .ts,.tsx --fix --cache src",
    "postinstall": "husky install",
    "prettier:check": "prettier --check **/*.{ts,tsx,js,json}",
    "prettier:fix": "prettier --write **/*.{ts,tsx,js,json}"
  },
  "dependencies": {
    "next": "^11.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "@types/node": "^16.10.3",
    "@types/react": "^17.0.27",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.3.7",
    "eslint": "^8.0.0",
    "eslint-config-marine": "^9.0.6",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "postcss": "^8.3.9",
    "prettier": "^2.4.1",
    "tailwindcss": "^2.2.16",
    "typescript": "^4.4.3"
  },
  "lint-staged": {
    "*.tsx": "eslint --fix --cache",
    "*.{ts,tsx,js,json}": "prettier --write"
  },
  "packageManager": "yarn@3.1.0"
}
