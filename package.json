{
	"name": "almeidx",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "next build --experimental-next-config-strip-types --webpack",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
		"deploy:cf": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"dev": "next dev --experimental-next-config-strip-types",
		"fmt": "biome check --write",
		"postinstall": "next typegen",
		"lint": "biome check",
		"preview:cf": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"start": "next start --experimental-next-config-strip-types"
	},
	"dependencies": {
		"@opennextjs/cloudflare": "^1.13.1",
		"clsx": "^2.1.1",
		"next": "^16.0.3",
		"next-themes": "^0.4.6",
		"react": "^19.2.0",
		"react-dom": "^19.2.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.7",
		"@tailwindcss/postcss": "^4.1.17",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.6",
		"@types/react-dom": "^19.2.3",
		"babel-plugin-react-compiler": "^1.0.0",
		"postcss": "^8.5.6",
		"tailwindcss": "^4.1.17",
		"typescript": "~5.9.3",
		"wrangler": "^4.50.0"
	},
	"packageManager": "pnpm@10.23.0",
	"pnpm": {
		"minimumReleaseAge": 1440,
		"onlyBuiltDependencies": [
			"@biomejs/biome",
			"@tailwindcss/oxide",
			"esbuild",
			"rclone.js",
			"sharp",
			"workerd"
		]
	}
}
